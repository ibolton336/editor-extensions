{
  "name": "konveyor",
  "publisher": "Konveyor",
  "displayName": "konveyor",
  "description": "VSCode Extension for Konveyor to assist in migrating and modernizing applications.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "konveyor.konveyorGUIView",
        "title": "Open GUI",
        "category": "Konveyor"
      },
      {
        "command": "konveyor.toggleFullScreen",
        "title": "Toggle GUI Fullscreen",
        "category": "Konveyor"
      }
    ],
    "view/title": [
      {
        "command": "konveyor.toggleFullScreen",
        "group": "navigation@1",
        "when": "view == konveyor.konveyorGUIView"
      }
    ],
    "editor/title": [
      {
        "command": "konveyor.toggleFullScreen",
        "group": "navigation@1",
        "when": "activeWebviewPanelId == konveyor.konveyorGUIView"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "konveyor",
          "title": "Konveyor",
          "icon": "$(extensions)"
        }
      ]
    },
    "views": {
      "konveyor": [
        {
          "type": "webview",
          "id": "konveyor.konveyorGUIView",
          "name": "Konveyor",
          "visibility": "visible"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/uuid": "^10.0.0",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.9.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "uuid": "^10.0.0"
  }
}
{
  "name": "konveyor-gui",
  "displayName": "Konveyor GUI",
  "description": "GUI for Konveyor",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "views": {
      "konveyor": [
        {
          "type": "webview",
          "id": "konveyor.konveyorGUIView",
          "name": "Konveyor GUI"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "build-react": "cd react && webpack --mode production"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "ts-loader": "^9.4.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "@vscode/test-electron": "^2.2.0"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0"
  }
}
